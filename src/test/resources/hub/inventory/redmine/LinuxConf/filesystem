/dev/mapper/vg_redmine-lv_root /                       ext4    defaults        1 1
UUID=f84aa6a9-1494-4078-9a28-f675adbaf7e2 /boot                   ext4    defaults        1 2
/dev/mapper/vg_redmine-lv_swap swap                    swap    defaults        0 0
tmpfs                   /dev/shm                tmpfs   defaults        0 0
devpts                  /dev/pts                devpts  gid=5,mode=620  0 0
sysfs                   /sys                    sysfs   defaults        0 0
proc                    /proc                   proc    defaults        0 0
